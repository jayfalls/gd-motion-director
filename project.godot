; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot UI Animation Framework"
config/features=PackedStringArray("4.0", "Forward Plus")

[autoload]

BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
gd="*res://addons/godot-action/utils_gd_action.tscn"

[display]

window/size/viewport_width=800
window/size/viewport_height=600

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-action/plugin.cfg", "res://addons/ui-anim-framework/plugin.cfg", "res://addons/beehave/plugin.cfg")
